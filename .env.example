# 86'd Game Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# EXPO / EAS BUILD
# ===========================================

# Expo Access Token (required for CI/CD builds)
# Get yours at: https://expo.dev/accounts/[your-account]/settings/access-tokens
EXPO_TOKEN=your_expo_token_here

# ===========================================
# APP STORE SUBMISSION (optional)
# ===========================================

# Apple ID for App Store Connect
APPLE_ID=

# App Store Connect App ID
ASC_APP_ID=

# Apple Developer Team ID
APPLE_TEAM_ID=

# ===========================================
# API KEYS (optional)
# ===========================================

# AI Mentor API endpoint (if using external AI)
AI_API_URL=

# Analytics (if using)
ANALYTICS_KEY=
