{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true,
      "claude/*": true
    }
  },
  "github": {
    "autoAlias": true
  }
}
